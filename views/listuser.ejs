<h2>All Users</h2>

<table border="1">
  <tr>
    <th>Username</th>
    <th>First</th>
    <th>Last</th>
    <th>Email</th>
  </tr>

  <% users.forEach(function(user) { %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.first %></td>
      <td><%= user.last %></td>
      <td><%= user.email %></td>
    </tr>
  <% }) %>
</table>
